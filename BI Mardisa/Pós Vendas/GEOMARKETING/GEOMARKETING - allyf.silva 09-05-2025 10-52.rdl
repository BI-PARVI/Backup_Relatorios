    <DataSet Name="Venda_Itens">
      <Query>
        <DataSourceName>Dw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@NOME_EMPRESA">
            <Value>=Parameters!NOME_EMPRESA.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ANO">
            <Value>=Parameters!ANO.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MES_ANO">
            <Value>=Parameters!MES_ANO.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OPERACAO_DESC">
            <Value>=Parameters!OPERACAO_DESC.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@GRUPO_DESC">
            <Value>=Parameters!GRUPO_DESC.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  PRVf_Venda_Itens_Report.EMPRESA_CODIGO
  ,PRVf_Venda_Itens_Report.NOME_EMPRESA
  ,PRVf_Venda_Itens_Report.OPERACAO_DESC
  ,PRVf_Venda_Itens_Report.VENDEDOR
  ,PRVf_Venda_Itens_Report.NF_NUMERO
  ,PRVf_Venda_Itens_Report.ITEM_CODIGO
  ,PRVf_Venda_Itens_Report.ITEM_DESC
  ,PRVf_Venda_Itens_Report.GRUPO_DESC
  ,PRVf_Venda_Itens_Report.ITEM_QTDE
  ,PRVf_Venda_Itens_Report.VALOR_VENDA_FINAL
  ,PRVf_Venda_Itens_Report.ULTIMA_COMPRA
  ,PRVf_Venda_Itens_Report.DATA_VENDA
  ,PRVf_Venda_Itens_Report.SEQUENCIA
  ,PRVf_Venda_Itens_Report.CLIENTE_NOTA
  ,PRVf_Venda_Itens_Report.CLIENTE_NOTA_CODIGO
  ,PRVf_Venda_Itens_Report.END_CID_UF
  ,PRVf_Venda_Itens_Report.END_CID_DESCRICAO
  ,PRVf_Venda_Itens_Report.ANO
  ,PRVf_Venda_Itens_Report.MES
  ,PRVf_Venda_Itens_Report.MES_ANO
 , CONCAT(SUBSTRING(PRVf_Venda_Itens_Report.MES_ANO,1,3),'/',substring(CONVERT(VARCHAR(4),PRVf_Venda_Itens_Report.ANO),3,4)) as ANO_MES
,PRVf_Venda_Itens_Report.CPF_CNPJ
FROM
  PRVf_Venda_Itens_Report
WHERE
  SUBSTRING(PRVf_Venda_Itens_Report.NOME_EMPRESA,17,LEN(PRVf_Venda_Itens_Report.NOME_EMPRESA)) in ( @NOME_EMPRESA)
  AND PRVf_Venda_Itens_Report.ANO in (@ANO)
  AND PRVf_Venda_Itens_Report.MES_ANO in (@MES_ANO)
  AND PRVf_Venda_Itens_Report.OPERACAO_DESC in (@OPERACAO_DESC)
  AND PRVf_Venda_Itens_Report.GRUPO_DESC in (@GRUPO_DESC)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
